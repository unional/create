{
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": [
      "es2015"
    ],
    "moduleResolution": "node",
    "newLine": "LF",
    "noUnusedLocals": true,
    "outDir": ".nowhere",
    "removeComments": false,
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true
  },
  "include": [
    "src",
    "typings"
  ]
}
